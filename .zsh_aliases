alias stress="for i in {1..5}; do; npm start; done"
alias update-branch="if [[ "$PWD" =~ app ]]; then git fetch origin dev && git merge origin/dev; elif [[ "$PWD" =~ qa-api-utils ]]; then git fetch origin master && git merge origin/master; else echo 'Unknown Directory'; fi"
alias bs="npm run build; npm start"
alias clean="rm -rf node_modules/ dist/; npm install"
alias ll="ls -lrt"
